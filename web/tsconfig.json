{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "moduleDetection": "auto",
    "baseUrl": "./",
    "paths": {
      "@comfyUI/*": ["../../../web/*"],
      "@/*": ["*"]
    },
    // "jsx": "preserve",
    // "jsxFactory": "",
    // "jsxFragmentFactory": "",
    // "typeRoots": [],
    // "types": [],
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["types", "src"]
}
